    "vscode-neovim.neovimInitVimPaths.darwin": "$HOME/.config/nvim-vscode/init.lua",
    "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.linux": "$HOME/.config/nvim-vscode/init.lua",
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "sql-formatter.uppercase": true,
    "sqlfluff.dialect": "snowflake",
    "terminal.integrated.shellIntegration.suggestEnabled": true,
    "editor.accessibilitySupport": "off",
    "glassit.alpha": 220,
    "settingsSync.ignoredExtensions": [
        "vscodevim.vim",
        "asvetliakov.vscode-neovim"
    ],
    "python.terminal.activateEnvInCurrentTerminal": true,
    "python.analysis.completeFunctionParens": true,
    "editor.tabCompletion": "on",
    "snowflake.snowsqlConfigPath": "~/.snowsql/config",
    "xml.server.vmargs": "-Xmx256M",
    "go.gopath": "/Users/vanducng/go",
    "go.alternateTools": {},
    "go.inferGopath": false,
    "git.openRepositoryInParentFolders": "always",
    "ansibleVault.keyfile": "~/.vault_pass.txt",
    "vs-kubernetes": {
        "vscode-kubernetes.minikube-path-mac": "/Users/vanducng/.vs-kubernetes/tools/minikube/darwin-arm64/minikube"
    },
    "window.zoomLevel": 2,
    "accessibility.signals.lineHasError": {
